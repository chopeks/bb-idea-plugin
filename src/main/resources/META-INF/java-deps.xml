<idea-plugin version="2">
  <extensions defaultExtensionNs="com.intellij">
      <moduleType id="SQUIRREL_MODULE" implementationClass="com.chopeks.SquirrelModuleType"/>
      <moduleConfigurationEditorProvider implementation="com.chopeks.configuration.SquirrelModuleEditorsProvider" order="first"/>
      <projectStructureDetector implementation="com.chopeks.project.SquirrelProjectStructureDetector"/>
      <projectService serviceInterface="com.chopeks.sdk.SquirrelSdkService" serviceImplementation="com.chopeks.sdk.SquirrelIdeaSdkService" overrides="true"/>
  </extensions>
</idea-plugin>
