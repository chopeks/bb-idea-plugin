<idea-plugin version="2">
  <extensions defaultExtensionNs="com.intellij">
    <moduleType id="SQUIRREL_MODULE" implementationClass="com.sqide.SquirrelModuleType"/>
    <moduleConfigurationEditorProvider implementation="com.sqide.configuration.SquirrelModuleEditorsProvider" order="first"/>
    <editorNotificationProvider implementation="com.sqide.inspections.WrongModuleTypeNotificationProvider"/>
    <projectStructureDetector implementation="com.sqide.project.SquirrelProjectStructureDetector"/>
    <!--<projectService serviceInterface="com.sqidekService" serviceImplementation="com.goide.sdk.GoIdeaSdkService" overrides="true"/>-->
  </extensions>
</idea-plugin>
